{
  "schemaVersion": 1,
  "id": "carpetskyadditions",
  "version": "${version}",
  "name": "${name}",
  "description": "Empty world generation with new ways to obtain resources",
  "authors": [
    "jsorrell"
  ],
  "contact": {
    "homepage": "https://github.com/jsorrell/CarpetSkyAdditions",
    "sources": "https://github.com/jsorrell/CarpetSkyAdditions",
    "issues": "https://github.com/jsorrell/CarpetSkyAdditions/issues"
  },
  "license": "MIT",
  "icon": "assets/carpetskyadditions/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.jsorrell.carpetskyadditions.SkyAdditionsExtension"
    ],
    "modmenu": [
      "com.jsorrell.carpetskyadditions.config.ModMenuIntegration"
    ]
  },
  "mixins": [
    "carpetskyadditions.mixins.json"
  ],
  "accessWidener": "carpetskyadditions.accesswidener",
  "depends": {
    "fabricloader": "*",
    "minecraft": "${mc_compatibility}",
    "carpet": "*",
    "fabric-object-builder-api-v1": "*",
    "fabric-registry-sync-v0": "*",
    "fabric-resource-loader-v0": "*",
    "fabric-transitive-access-wideners-v1": "*",
    "cloth-config2": "*"
  },
  "suggests": {
    "modmenu": "*"
  }
}
